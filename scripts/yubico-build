#!/bin/bash
set -xa

if [ -f ../sdb.env ]; then
  export $(echo $(cat ../sdb.env | sed 's/#.*//g'| xargs) | envsubst)
fi
if [ -f yub.env ]; then
  export $(echo $(cat ../yub.env | sed 's/#.*//g'| xargs) | envsubst)
fi
set +a

cd ../yubico-net-sdk/Yubico.NativeShims
/bin/bash build-${YUBICO_BUILD_ENV}.sh

docker tag ${SDB_SOL_DOCKER} ${SDB_SOL_DOCKER_IMG}:latest
docker build push ${SDB_SOL_DOCKER_IMG}:latest

