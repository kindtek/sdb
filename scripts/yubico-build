#!/bin/bash
#/bin/bash
set -ax
cd ../yubico-net-sdk
. sdb.ini
set +a

cd Yubico.NativeShims
docker login -u="${DOCKER_USERNAME}" -p="${DOCKER_PASSWORD}"
/bin/bash build-${YUBICO_BUILD_ENV}.sh

docker tag  "${SDB_YUB_DOCKER_IMG}:${SDB_YUB_DOCKER_TAG}"  "${SDB_YUB_DOCKER_IMG}:latest"
docker push "${SDB_YUB_DOCKER_IMG}:${SDB_YUB_DOCKER_TAG}"