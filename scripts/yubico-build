#!/bin/bash
set -ax
cd ../solana
. ../sdb.ini
. ../sdb_yub.ini
set +a

cd ../yubico-net-sdk/Yubico.NativeShims
YUBICO_ARTIFACT_DIR=${YUBICO_ARTIFIACT_DIR} SDB_YUB_DOCKER_IMG=${SDB_YUB_DOCKER_IMG} SDB_YUB_DOCKER=${SDB_YUB_DOCKER} \
/bin/bash build-${YUBICO_BUILD_ENV}.sh 

docker build ${SDB_YUB_DOCKER}
set +a