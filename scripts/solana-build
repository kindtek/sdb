#!/bin/bash
d ..
set -a
source <(cat ../sdb.env | sed -e '/^#/d;/^\s*$/d' -e "s/'/'\\\''/g" -e "s/=\(.*\)/='\1'/g")
source <(cat ../yub.env | sed -e '/^#/d;/^\s*$/d' -e "s/'/'\\\''/g" -e "s/=\(.*\)/='\1'/g")
set +a

cd solana
CHANNEL=latest CI=true /bin/bash sdk/docker-solana/build.sh
docker push kindtek/solana-safedb-debian:latest

docker tag ${SDB_SOL_DOCKER} ${SDB_SOL_DOCKER_IMG}:latest
docker build push ${SDB_SOL_DOCKER_IMG}:latest