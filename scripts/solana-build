#!/bin/bash
set -ax
cd ../solana
# /bin/bash 
. ci/check-channel-version.sh
export STABLE_CHANNEL_LATEST_TAG=$STABLE_CHANNEL_LATEST_TAG
. ../sdb.env STABLE_CHANNEL_LATEST_TAG=$STABLE_CHANNEL_LATEST_TAG
. ../sol.env STABLE_CHANNEL_LATEST_TAG=$STABLE_CHANNEL_LATEST_TAG
set +a

cd ../solana
/bin/bash sdk/docker-solana/build.sh
docker push kindtek/solana-safedb-debian:latest

docker tag ${SDB_SOL_DOCKER} ${SDB_SOL_DOCKER_IMG}:latest
docker push ${SDB_SOL_DOCKER_IMG}:latest